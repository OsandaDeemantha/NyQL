{
  "activate": "mysql",

  "translators": [
    "com.virtusa.gto.insight.nyql.db.mysql.MySqlFactory"
  ],

  "caching": {
    "compiledScripts": true,
    "generatedQueries": true
  },

  "defaultImports": [
    "com.virtusa.gto.insight.nyql.ddl.DKeyIndexType",
    "com.virtusa.gto.insight.nyql.ddl.DFieldType",
    "com.virtusa.gto.insight.nyql.ddl.DReferenceOption",
    "com.virtusa.gto.insight.nyql.utils.QueryType",
    "java.sql.JDBCType"
  ],

  "defaultExecutor": "jdbc",
  "executors": [
    {
      "name": "jdbc",
      "factory": "com.virtusa.gto.insight.nyql.engine.impl.QJdbcExecutorFactory",

      "url": "jdbc:mysql://localhost/sakila",
      "username": "root",
      "password": "",

      "pooling": {
        "maximumPoolSize": 1,
        "prepStmtCacheSize": 300,
        "prepStmtCacheSqlLimit": 2048,
        "useServerPrepStmts": true
      }
    }
  ],

  "defaultRepository": "default",
  "repositories": [
    {
      "name": "default",
      "repo": "com.virtusa.gto.insight.nyql.engine.repo.QRepositoryImpl",
      "mapper": "com.virtusa.gto.insight.nyql.engine.repo.QScriptsFolder",
      "mapperArgs": {
        "baseDir": "./scripts"
      }
    }
  ]

}